<body style ="background-color: #A4C8EC";>
 <div class="navbar"></div>
 <div class="container"></div>
 <h5>LIKED USER LIST</h5>
 <div class="table-responsive">
   <table class ="table">
    <tr>
      <th>First Name</th><td></td>
      <th>Last Name</th><td></td>
    </tr>
    <% @likeships.each do|like| %>
    <tr>
      <td><%= User.find(like.user_id).firstname%></td><td></td>
      <td><%= User.find(like.user_id).lastname%></td>
      <td><%= link_to "Unlike",like, method: :delete %></td>
      <td></td>
      <%end%>
    </tr>
    </table>
	</div>
  <%=link_to 'Back',:back,class: "btn btn-primary btn-xs"%>
</body>

